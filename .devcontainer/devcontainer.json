{
    "name": "Datadog-Agent-DevEnv",
    "image": "agentdevamd64",
    "runArgs": [
        "--cap-add=SYS_PTRACE",
        "--security-opt",
        "seccomp=unconfined"
    ],
    "remoteUser": "datadog",
    "customizations": {
        "vscode": {
            "settings": {
                "go.toolsManagement.checkForUpdates": "local",
                "go.useLanguageServer": true,
                "go.gopath": "/home/datadog/go",
                "go.goroot": "/usr/local/go",
                "go.buildTags": "consul,zk,gce,otlp,ec2,kubeapiserver,process,python,zlib,orchestrator,apm,etcd,jmx,secrets,kubelet",
                "go.testTags": "consul,zk,gce,otlp,ec2,kubeapiserver,process,python,zlib,orchestrator,apm,etcd,jmx,secrets,kubelet"
            },
            "extensions": [
                "golang.Go"
            ]
        }
    }
}